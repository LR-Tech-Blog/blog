---
import { Icon } from "astro-icon/components";
interface Props {
    brand: string
}

const { brand } = Astro.props
---
<Icon name={`simple-icons:${brand}`} size={25} class="fill-text dark:fill-dark_text"/>

<style>
    [data-icon] {
        display: inline-block;
        vertical-align: middle;
    }
</style>