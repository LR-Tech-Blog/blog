---
import Icon from "astro-icon"

interface Props {
    name: string,
    width: string,
    height?: string
    alt: string
}

const { name, width, height, alt } = Astro.props
---

<button class="transition hover:scale-110 focus:ring-2" aria-label={alt}>
    <Icon name={name} width={width} height={height}/>
</button>